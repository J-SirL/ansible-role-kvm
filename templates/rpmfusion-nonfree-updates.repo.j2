[rpmfusion-nonfree-updates]
name=RPM Fusion for EL $releasever - Nonfree - Updates
#baseurl=http://download1.rpmfusion.org/nonfree/el/updates/$releasever/$basearch/
metalink=http://mirrors.rpmfusion.org/metalink?repo=nonfree-el-updates-released-$releasever&arch=$basearch
enabled=1
type=rpm-md
gpgcheck=1
repo_gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-nonfree-el-$releasever

[rpmfusion-nonfree-updates-debuginfo]
name=RPM Fusion for EL $releasever - Nonfree - Updates Debug
#baseurl=http://download1.rpmfusion.org/nonfree/el/updates/$releasever/$basearch/debug/
metalink=http://mirrors.rpmfusion.org/metalink?repo=nonfree-el-updates-released-debug-$releasever&arch=$basearch
enabled=0
type=rpm-md
gpgcheck=1
repo_gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-nonfree-el-$releasever

[rpmfusion-nonfree-updates-source]
name=RPM Fusion for EL $releasever - Nonfree - Updates Source
#baseurl=http://download1.rpmfusion.org/nonfree/el/updates/$releasever/SRPMS/
metalink=http://mirrors.rpmfusion.org/metalink?repo=nonfree-el-updates-released-source-$releasever&arch=$basearch
enabled=0
type=rpm-md
gpgcheck=1
repo_gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-nonfree-el-$releasever

